{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@angular/*": [
        "./node_modules/@angular/*"
      ],
      "firebase/*": [
        "./node_modules/firebase/*"
      ],
      "@firebase/*": [
        "./node_modules/@firebase/*"
      ],
      "@ionic/*": [
        "./node_modules/@ionic/*"
      ],
      "ionngx-firebase-auth-ui": [
        "dist/ionngx-firebase-auth-ui/ionngx-firebase-auth-ui",
        "dist/ionngx-firebase-auth-ui"
      ],
      "font-icon-css": [
        "./node_modules/font-icon-css"
      ]
    }
  }
}
